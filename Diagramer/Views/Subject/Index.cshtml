@model List<Subject>; 
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1>Мои предметы</h1>
    <a asp-action="AddSubject">Создать предмет</a>
</div>
@foreach (var subject in @Model)
{
    <h2><a href='@Url.Action("OpenSubject", new {id = subject.Id})'>@subject.Name</a></h2>
}